{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "rimraf dist && tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gen-migration": "yarn typeorm migration:generate -n",
    "migration-all": "yarn typeorm migration:run",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "doc": "npx compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@compodoc/compodoc": "^1.1.10",
    "@godaddy/terminus": "^4.1.2",
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.5.3",
    "@nestjs/platform-socket.io": "^6.5.3",
    "@nestjs/swagger": "^4.2.8",
    "@nestjs/terminus": "^6.5.0",
    "@nestjs/typeorm": "^6.1.3",
    "@nestjs/websockets": "^6.5.3",
    "@nestjsx/crud": "^4.2.0",
    "@nestjsx/crud-typeorm": "^4.2.0",
    "@sentry/node": "^5.12.3",
    "@types/socket.io": "^2.1.2",
    "bcrypt": "^3.0.6",
    "cache-manager": "^2.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "cls-hooked": "^4.2.2",
    "dotenv": "^8.0.0",
    "faker": "^4.1.0",
    "helmet": "^3.21.2",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "mysql2": "^1.6.5",
    "nest-access-control": "^2.0.1",
    "nestjs-redis": "^1.2.2",
    "nestjs-typeorm-paginate": "^1.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.2",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "2.0.7",
    "cross-env": "^5.2.0",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
